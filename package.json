{
  "name": "codeinterpreterts_mcp",
  "version": "1.0.0",
  "main": "./build/index.js",
  "bin": {
    "code-interpreter": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "dev": "tsx src/index.ts",
    "start": "node build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest tests/security",
    "test:mcp": "node tests/test-mcp.mjs",
    "test:enhanced": "node tests/test-enhanced.mjs",
    "test:communication": "node tests/test-mcp-communication.mjs",
    "test:all": "npm run test && npm run test:mcp && npm run test:enhanced && npm run test:communication",
    "clean": "rm -rf build/ && rm -rf tmp/",
    "excel:create": "node scripts/create-excel-test.cjs",
    "excel:read": "node scripts/read-excel-test.cjs",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "typescript",
    "code-interpreter"
  ],
  "author": "",
  "license": "ISC",
  "description": "TypeScript Code Interpreter MCP Server",
  "files": [
    "build"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.1",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "jest": "^29.7.0",
    "tsx": "^4.19.4",
    "xlsx": "^0.18.5",
    "zod": "^3.25.28"
  }
}
